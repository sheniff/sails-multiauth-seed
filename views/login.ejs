<h1>Login</h1>
<form action='/login' method='post'>
  <div>
    <label>Email:</label>
    <input type='text' name='email' >
  </div>
  <div>
    <label>Password:</label>
    <input type='password' name='password' >
  </div>
  <input type='submit' value='Log In'>
</form>

<h1>Sign Up</h1>
<form action='/signup' method='post'>
  <div>
    <label>Email:</label>
    <input type='text' name='email' >
  </div>
  <div>
    <label>Password:</label>
    <input type='password' name='password' >
  </div>
  <input type='submit' value='Log In'>
</form>

<a href="/auth/facebook" title="Use Facebook!">Use Facebook!</a>
<a href="/auth/google" title="Use Google!">Use Google!</a>
<a href="/auth/twitter" title="Use Twitter!">Use Twitter!</a>

